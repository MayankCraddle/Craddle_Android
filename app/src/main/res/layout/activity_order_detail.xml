<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/whiteCream">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <include layout="@layout/custom_toolbar_user" />

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_6sdp"
                    app:cardCornerRadius="@dimen/_5sdp">

                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:id="@+id/tvOrderId"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/_9sdp" />

                        <RelativeLayout
                            android:id="@+id/ivOpenProductDetail"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <androidx.appcompat.widget.AppCompatImageView
                                android:id="@+id/iv_product_item"
                                android:layout_width="@dimen/_50sdp"
                                android:layout_height="@dimen/_60sdp"
                                android:layout_centerVertical="true"
                                android:scaleType="centerCrop"
                                android:src="@drawable/loading" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/_15sdp"
                                android:layout_toRightOf="@+id/iv_product_item"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:id="@+id/tv_product_name"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tv_quantity"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_8sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tvSeller"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_8sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tv_price"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_20sdp">

                            <LinearLayout
                                android:id="@+id/ll_processing_order"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <View
                                    android:id="@+id/viewConfirmedDot"
                                    android:layout_width="@dimen/_10sdp"
                                    android:layout_height="@dimen/_10sdp"
                                    android:background="@drawable/shap_circel_gray" />

                                <View
                                    android:id="@+id/viewConfirmedTrack"
                                    android:layout_width="@dimen/_2ssp"
                                    android:layout_height="@dimen/_20sdp"
                                    android:layout_gravity="center"
                                    android:background="@color/gray" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_toEndOf="@+id/ll_processing_order"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/order_confirmed"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tvCreateOn"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:id="@+id/ll_processing_order_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <View
                                    android:id="@+id/viewPackedDot"
                                    android:layout_width="@dimen/_10sdp"
                                    android:layout_height="@dimen/_10sdp"
                                    android:background="@drawable/shap_circel_gray" />

                                <View
                                    android:id="@+id/viewPackedTrack"
                                    android:layout_width="2dp"
                                    android:layout_height="29dp"
                                    android:layout_gravity="center"
                                    android:background="@color/gray" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_toEndOf="@+id/ll_processing_order_1"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Packed"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tvPackedDate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:id="@+id/ll_order_shipped"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <View
                                    android:id="@+id/viewShippedDot"
                                    android:layout_width="@dimen/_10sdp"
                                    android:layout_height="@dimen/_10sdp"
                                    android:background="@drawable/shap_circel_gray" />

                                <View
                                    android:id="@+id/viewShippedTrack"
                                    android:layout_width="@dimen/_2ssp"
                                    android:layout_height="@dimen/_20sdp"
                                    android:layout_gravity="center"
                                    android:background="@color/gray" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_toEndOf="@+id/ll_order_shipped"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Shipped"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tvShippedDate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_8sdp" />
                            </LinearLayout>
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <LinearLayout
                                android:id="@+id/ll_order_dele"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <View
                                    android:id="@+id/viewDeliveredDot"
                                    android:layout_width="@dimen/_10sdp"
                                    android:layout_height="@dimen/_10sdp"
                                    android:background="@drawable/shap_circel_gray" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_toEndOf="@+id/ll_order_dele"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/delivered_new"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp" />

                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/tv_deliveredDate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text=""
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </RelativeLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1ssp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:background="@color/whiteCream"
                            android:visibility="gone" />

                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <ImageView
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                android:src="@drawable/invoice_icon" />

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_9sdp"
                                android:text="@string/download_invoice" />

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:layout_width="@dimen/_20sdp"
                                    android:layout_height="@dimen/_20sdp"
                                    android:layout_alignParentRight="true"
                                    android:src="@drawable/arrow_right_new" />
                            </RelativeLayout>


                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <LinearLayout
                            android:id="@+id/llRating"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RatingBar
                                android:id="@+id/rBar_product_details"
                                style="@android:style/Widget.Material.RatingBar.Small"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:numStars="5"
                                android:stepSize="0.5"
                                android:theme="@style/RatingBar" />

                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:id="@+id/tvRating"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="@color/light_grey" />

                        <LinearLayout
                            android:id="@+id/llCancelOrder"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:foreground="?attr/selectableItemBackgroundBorderless"
                            android:orientation="horizontal"
                            android:visibility="gone"
                            android:paddingTop="@dimen/_5sdp">

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:id="@+id/tvCancelOrder"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Cancel Order"
                                android:textColor="@color/black" />

                            <ImageView
                                android:layout_width="@dimen/_15sdp"
                                android:layout_height="@dimen/_15sdp"
                                android:src="@drawable/arrow_right_new" />
                        </LinearLayout>
                    </androidx.appcompat.widget.LinearLayoutCompat>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:cardCornerRadius="@dimen/_5sdp">

                    <LinearLayout
                        android:id="@+id/cv_container"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4sdp"
                        android:foreground="?attr/selectableItemBackgroundBorderless"
                        android:orientation="vertical">


                        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:layout_marginRight="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_8sdp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/parentLayout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:orientation="horizontal">


                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/_8sdp"
                                        android:orientation="vertical">

                                        <androidx.appcompat.widget.LinearLayoutCompat
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                                android:id="@+id/tv_land_mark"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:layout_weight="1"
                                                android:gravity="start"
                                                android:text="@string/shipping_details"
                                                android:textAlignment="viewStart"
                                                android:textAllCaps="false"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_12ssp" />

                                            <LinearLayout
                                                android:id="@+id/more_button"
                                                android:layout_width="@dimen/_17sdp"
                                                android:layout_height="@dimen/_12sdp"
                                                android:layout_gravity="center"
                                                android:gravity="center"
                                                android:textColor="@color/white"
                                                android:textSize="@dimen/_14ssp">

                                                <ImageView
                                                    android:id="@+id/iv_more"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_gravity="center"
                                                    android:src="@drawable/more"
                                                    android:visibility="gone" />
                                            </LinearLayout>
                                        </androidx.appcompat.widget.LinearLayoutCompat>


                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                                android:id="@+id/tvCustomerName"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center_vertical"
                                                android:gravity="start"
                                                android:text="Home"
                                                android:textAlignment="viewStart"
                                                android:textAllCaps="false"
                                                android:textColor="@color/black"
                                                android:textSize="@dimen/_11ssp" />

                                            <RelativeLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content">

                                                <LinearLayout
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginStart="@dimen/_10sdp"
                                                    android:background="@color/whiteCream"
                                                    android:padding="@dimen/_1sdp">

                                                    <com.cts.saharaGoSeller.customui.CustomTextView
                                                        android:id="@+id/tv_land_title"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_gravity="center_vertical"
                                                        android:layout_marginStart="@dimen/_5sdp"
                                                        android:layout_marginEnd="@dimen/_5sdp"
                                                        android:layout_weight="1"
                                                        android:text="Home" />
                                                </LinearLayout>

                                                <LinearLayout
                                                    android:id="@+id/ll_default"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_alignParentRight="true"
                                                    android:layout_marginStart="@dimen/_10sdp"
                                                    android:padding="@dimen/_1sdp"
                                                    android:visibility="gone">

                                                    <ImageView
                                                        android:id="@+id/tv_default"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_gravity="center_vertical"
                                                        android:layout_marginStart="@dimen/_5sdp"
                                                        android:layout_marginEnd="@dimen/_5sdp"
                                                        android:layout_weight="1"
                                                        android:src="@drawable/default_address" />
                                                </LinearLayout>
                                            </RelativeLayout>

                                            <LinearLayout
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:orientation="horizontal">

                                                <LinearLayout
                                                    android:id="@+id/edit_button"
                                                    android:layout_width="@dimen/_17sdp"
                                                    android:layout_height="@dimen/_17sdp"
                                                    android:layout_gravity="center"
                                                    android:layout_marginRight="@dimen/_5sdp"
                                                    android:gravity="center"
                                                    android:textColor="@color/white"
                                                    android:textSize="@dimen/_14ssp"
                                                    android:visibility="gone">

                                                    <ImageView
                                                        android:id="@+id/iv_edit_address"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_gravity="center"
                                                        android:src="@drawable/edit_dark" />
                                                </LinearLayout>


                                                <LinearLayout
                                                    android:id="@+id/delete_button"
                                                    android:layout_width="@dimen/_17sdp"
                                                    android:layout_height="@dimen/_17sdp"
                                                    android:layout_gravity="center"
                                                    android:gravity="center"
                                                    android:textColor="@color/white"
                                                    android:textSize="@dimen/_14ssp"
                                                    android:visibility="gone">

                                                    <ImageView
                                                        android:id="@+id/iv_delete_add"
                                                        android:layout_width="wrap_content"
                                                        android:layout_height="wrap_content"
                                                        android:layout_gravity="center"
                                                        android:src="@drawable/delete_dark" />
                                                </LinearLayout>


                                            </LinearLayout>
                                        </LinearLayout>

                                        <com.cts.saharaGoSeller.customui.CustomTextView
                                            android:id="@+id/tvAddress"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="1"
                                            android:gravity="start"
                                            android:text=""
                                            android:textAlignment="viewStart"
                                            android:textAllCaps="false"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_10ssp" />

                                        <com.cts.saharaGoSeller.customui.CustomTextView
                                            android:id="@+id/tvPhoneNumber"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="start"
                                            android:maxLines="1"
                                            android:text=""
                                            android:textAlignment="viewStart"
                                            android:textAllCaps="false"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_10ssp" />

                                        <com.cts.saharaGoSeller.customui.CustomTextView
                                            android:id="@+id/tvCity"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="start"
                                            android:maxLines="1"
                                            android:text=""
                                            android:textAlignment="viewStart"
                                            android:textAllCaps="false"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_10ssp" />

                                    </LinearLayout>


                                </LinearLayout>


                            </LinearLayout>


                        </LinearLayout>


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:cardCornerRadius="@dimen/_6sdp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_8sdp">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/price_detail"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_11sdp" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_7sdp">

                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/subtotal"
                                android:textSize="@dimen/_11sdp" />

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:id="@+id/tvProductQty"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:text=""
                                android:textSize="@dimen/_11sdp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_7sdp">

                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/delivery_fee"
                                android:textSize="@dimen/_11sdp" />

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:id="@+id/tvCustomerNameShiping"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:text=""
                                android:textSize="@dimen/_11sdp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_7sdp">

                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/discount"
                                android:textSize="@dimen/_11sdp" />

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold

                                android:id="@+id/tvDiscount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:text=""
                                android:textSize="@dimen/_11sdp" />
                        </RelativeLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_1ssp"
                            android:background="@color/colorLightWhite" />

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/_7sdp">

                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/total_price"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_12sdp" />

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:id="@+id/tvTotalOrderPrice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginEnd="@dimen/_10sdp"
