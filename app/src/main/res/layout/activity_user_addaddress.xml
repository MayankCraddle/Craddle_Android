<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="onAddAdressClick"
            type="android.view.View.OnClickListener" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/whiteCream">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <include layout="@layout/custom_toolbar_user"
                   />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_15sdp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_marginTop="@dimen/_10ssp"
                            android:layout_width="wrap_content"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/first_name"
                                    android:textAllCaps="false"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10ssp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:background="@drawable/edittext_bg2"
                                    android:layout_height="wrap_content">

                                    <EditText
                                        android:id="@+id/edtFirstName"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:textAlignment="viewStart"
                                        android:inputType="textCapSentences"
                                        android:imeOptions="actionNext"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:paddingStart="@dimen/_10sdp"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_marginTop="@dimen/_10ssp"
                            android:layout_width="wrap_content"
                            android:layout_weight="1"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:layout_marginStart="@dimen/_15sdp"
                                android:gravity="center"
                                android:orientation="vertical">

                                <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/last_name"
                                    android:textAllCaps="false"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_10ssp" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:orientation="vertical"
                                    android:layout_marginTop="@dimen/_2sdp"
                                    android:background="@drawable/edittext_bg2"
                                    android:layout_height="wrap_content">

                                    <EditText
                                        android:id="@+id/edtLastName"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:textAlignment="viewStart"
                                        android:imeOptions="actionNext"
                                        android:inputType="textCapSentences"
                                        android:background="@null"
                                        android:singleLine="true"
                                        android:paddingStart="@dimen/_10sdp"
                                        android:textSize="@dimen/_10ssp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Email id"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_10ssp" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:background="@drawable/edittext_bg2"
                            android:layout_height="wrap_content">
                            <androidx.appcompat.widget.AppCompatEditText
                                android:id="@+id/edtEmail"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:gravity="center|start"
                                android:textAlignment="viewStart"
                                android:imeOptions="actionNext"
                                android:background="@null"
                                android:inputType="textCapSentences"
                                android:singleLine="true"
                                android:paddingStart="@dimen/_10sdp"
                                android:textSize="@dimen/_10ssp" />
                        </LinearLayout>
                    </LinearLayout>
                   <androidx.appcompat.widget.LinearLayoutCompat
                       android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/_20sdp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <ImageView
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                android:layout_gravity="center_vertical"
                                android:src="@drawable/mobile_black" />

                            <com.hbb20.CountryCodePicker
                                android:id="@+id/ccp"
                                app:ccp_textSize="@dimen/_10sdp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                app:ccp_arrowColor="@color/black"
                                app:ccp_autoDetectCountry="false"
                                app:ccp_defaultNameCode="NG"
                                app:ccp_autoDetectLanguage="false"
                                app:ccp_showFlag="false" />
                        </androidx.appcompat.widget.LinearLayoutCompat>
                       <LinearLayout

                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_gravity="center"
                           android:layout_weight="1"
                           android:gravity="center"
                           android:orientation="vertical">

                           <com.cts.saharaGoSeller.customui.CustomTextviewBold
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:text="@string/mobile_number"
                               android:textAllCaps="false"
                               android:textColor="@color/black"
                               android:textSize="@dimen/_10ssp" />
                           <LinearLayout
                               android:layout_width="match_parent"
                               android:orientation="horizontal"
                               android:layout_marginTop="@dimen/_2sdp"
                               android:background="@drawable/edittext_bg2"
                               android:layout_height="wrap_content">
                               <androidx.appcompat.widget.AppCompatEditText
                                   android:id="@+id/edtMobileNo"
                                   android:layout_width="match_parent"
                                   android:layout_height="@dimen/_30sdp"
                                   android:gravity="center|start"
                                   android:textAlignment="viewStart"
                                   android:imeOptions="actionNext"
                                   android:inputType="number"
                                   android:background="@null"
                                   android:singleLine="true"
                                   android:paddingStart="@dimen/_10sdp"
                                   android:textSize="@dimen/_10ssp" />
                           </LinearLayout>
                       </LinearLayout>
                    </androidx.appcompat.widget.LinearLayoutCompat>

                    <LinearLayout
                        android:id="@+id/llOpenAddress"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/street_address"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_10ssp" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:background="@drawable/edittext_bg2"
                            android:layout_height="wrap_content">

                            <androidx.appcompat.widget.AppCompatEditText
                                android:id="@+id/edtAddress"
                                android:visibility="gone"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:gravity="center|start"
                                android:textAlignment="viewStart"
                                android:imeOptions="actionNext"
                                android:background="@null"
                                android:inputType="textCapSentences"
                                android:singleLine="true"
                                android:paddingStart="@dimen/_10sdp"
                                android:textSize="@dimen/_10ssp" />
                            <com.cts.saharaGoSeller.customui.CustomTextView
                                android:id="@+id/tvAddressSugg"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:gravity="center|start"
                                android:textAlignment="viewStart"
                                android:imeOptions="actionNext"
                                android:background="@null"
                                android:singleLine="true"
                                android:paddingStart="@dimen/_10sdp"
                                android:textSize="@dimen/_10ssp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/landmark"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_10ssp" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:background="@drawable/edittext_bg2"
                            android:layout_height="wrap_content">
                            <androidx.appcompat.widget.AppCompatEditText
                                android:id="@+id/edtLandMark"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:gravity="center|start"
                                android:textAlignment="viewStart"
                                android:imeOptions="actionNext"
                                android:background="@null"
                                android:inputType="textCapSentences"
                                android:singleLine="true"
                                android:paddingStart="@dimen/_10sdp"
                                android:textSize="@dimen/_10ssp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/state"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <LinearLayout
                                android:id="@+id/llOpenState"
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/edittext_bg2"
                                android:layout_height="wrap_content">
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/edtState"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:gravity="center|start"
                                    android:textAlignment="viewStart"
                                    android:imeOptions="actionNext"
                                    android:background="@null"
                                    android:singleLine="true"
                                    android:paddingStart="@dimen/_10sdp"
                                    android:textSize="@dimen/_10ssp" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginStart="@dimen/_15sdp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/city"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <LinearLayout
                                android:id="@+id/llSelectCity"
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/edittext_bg2"
                                android:layout_height="wrap_content">
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/edtCity"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:gravity="center|start"
                                    android:textAlignment="viewStart"
                                    android:imeOptions="actionNext"
                                    android:background="@null"
                                    android:singleLine="true"
                                    android:paddingStart="@dimen/_10sdp"
                                    android:textSize="@dimen/_10ssp" />

                            </LinearLayout>
                        </LinearLayout>




                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="@dimen/_10ssp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/country"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/edittext_bg2"
                                android:layout_height="wrap_content">
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:id="@+id/edtLocality"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:gravity="center|start"
                                    android:textAlignment="viewStart"
                                    android:imeOptions="actionNext"
                                    android:background="@null"
                                    android:singleLine="true"
                                    android:text="Nigeria"
                                    android:paddingStart="@dimen/_10sdp"
                                    android:textSize="@dimen/_10ssp" />

                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginLeft="@dimen/_15sdp"
                            android:gravity="center"
                            android:orientation="vertical">

                            <com.cts.saharaGoSeller.customui.CustomTextviewBold
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/pin_code"
                                android:textAllCaps="false"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:background="@drawable/edittext_bg2"
                                android:layout_height="wrap_content">



                                <EditText
                                    android:id="@+id/edtZipcode"
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_30sdp"
                                    android:gravity="center|start"
                                    android:textAlignment="viewStart"
                                    android:imeOptions="actionNext"
                                    android:inputType="number"
                                    android:background="@null"
                                    android:singleLine="true"
                                    android:paddingStart="@dimen/_10sdp"
                                    android:textSize="@dimen/_10ssp" />
                            </LinearLayout>
                        </LinearLayout>




                    </LinearLayout>

                    <LinearLayout
                        android:visibility="gone"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/address"
                            android:textAllCaps="false"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_10ssp" />


                        <LinearLayout
                            android:visibility="gone"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_marginTop="@dimen/_2sdp"
                            android:background="@drawable/edittext_bg2"
                            android:layout_height="@dimen/_60sdp">



                            <androidx.appcompat.widget.AppCompatEditText
                                android:id="@+id/edtAddress1"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_30sdp"
                                android:gravity="center|start"
                                android:textAlignment="viewStart"
                                android:imeOptions="actionDone"
                                android:inputType="text"
                                android:background="@null"
                                android:paddingStart="@dimen/_10sdp"
                                android:textSize="@dimen/_10ssp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/type_of_address"
                            android:textColor="@color/black"
                            android:gravity="start"
                            android:textAlignment="viewStart"
                            android:textAllCaps="false"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:textSize="@dimen/_10ssp" />

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <LinearLayout
                                android:id="@+id/ll_home"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_margin="@dimen/_2sdp"
                                android:padding="@dimen/_5sdp"
                                android:background="@drawable/corner_rouded"
                                android:layout_height="wrap_content">
                                <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="@dimen/_15sdp"
                                    android:layout_height="@dimen/_15sdp"
                                    android:src="@drawable/home_address"/>
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_9sdp"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:text="@string/home"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/ll_office"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:padding="@dimen/_5sdp"
                                android:background="@drawable/corner_rounded_grey"
                                android:layout_height="wrap_content">
                                <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="@dimen/_15sdp"
                                    android:layout_height="@dimen/_15sdp"
                                    android:src="@drawable/office_address"/>
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    android:textSize="@dimen/_9sdp"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:text="@string/work"/>
                            </LinearLayout>
                            <LinearLayout
                                android:visibility="gone"
                                android:id="@+id/ll_other"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_margin="@dimen/_2sdp"
                                android:padding="@dimen/_5sdp"
                                android:background="@drawable/corner_rounded_grey"
                                android:layout_height="wrap_content">
                                <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="@dimen/_15sdp"
                                    android:layout_height="@dimen/_15sdp"
                                    android:src="@drawable/home_address"/>
                                <com.cts.saharaGoSeller.customui.CustomTextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="@dimen/_9sdp"
                                    android:textColor="@color/black"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:text="@string/other"/>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>


                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    </LinearLayout>

                </LinearLayout>
                <androidx.cardview.widget.CardView
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_margin="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    app:cardCornerRadius="@dimen/_7sdp">
                    <RelativeLayout
                        android:id="@+id/btn_AddAddress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_8sdp"
                        android:onClick="@{(v)->onAddAdressClick.onClick(v)}"
                        android:background="@color/colorGreen">

                        <com.cts.saharaGoSeller.customui.CustomTextviewBold
                            android:id="@+id/tvAddAdress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="@string/add_address"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_11sdp" />
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_10sdp"/>


              <!--  <com.saharogo.customui.CustomButton
                    android:id="@+id/btnMarkAsDefault"
                    android:layout_width="@dimen/_210sdp"
                    android:layout_height="@dimen/_35sdp"
                    android:layout_alignParentBottom="true"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_25sdp"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_marginEnd="@dimen/_25sdp"
                    android:layout_marginBottom="@dimen/_20sdp"
                    android:background="@drawable/button_bordered_bg"
                    android:gravity="center"
                    android:text="@string/mark_this_as_default_address"
                    android:textAllCaps="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_12ssp" />-->
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</layout>